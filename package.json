{
  "name": "psd.com",
  "version": "1.0.1",
  "description": "My personal portfolio website",
  "homepage": "https://github.com/PaleBluDot/PSD.com#readme",
  "bugs": "https://github.com/PaleBluDot/PSD.com/issues",
  "main": "index.html",
  "license": "MIT",
  "author": {
    "name": "Pavel Sanchez",
    "email": "info@pavelsanchez.design",
    "url": "https://pavelsanchez.design"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaleBluDot/PSD.com.git"
  },
  "keywords": [
    "portfolio",
    "website",
    "designer"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@tbranyen/jsdom": "^13.0.0",
    "concurrently": "^4.1.0",
    "html-minifier": "^4.0.0",
    "json-to-scss": "^1.3.1",
    "sass": "^1.21.0",
    "slugify": "^1.3.4",
    "stalfos": "git://github.com/andybelldesign/stalfos.git#c8971d22726326cfc04089b2da4d51eeb1ebb0eb"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  },
  "scripts": {
    "sass:tokens": "npx json-to-scss src/_data/tokens.json src/scss/_tokens.scss",
    "sass:process": "npm run sass:tokens && sass src/scss/global.scss src/_includes/assets/css/global.css --style=compressed",
    "start": "concurrently \"npm run sass:process -- --watch\" \"npm run serve\"",
    "serve": "cross-env ELEVENTY_ENV=development npx eleventy --serve",
    "prod": "npm run sass:process && npx eleventy"
  }
}
